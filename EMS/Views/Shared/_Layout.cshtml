                            <li><a id="hrefBudget" onclick="AtlasNavigation.toModule('Budget'); return false;" href="~/Budget/Import"><i class="fa fa-suitcase user01"></i> <span>Budget</span></a></li>
                            <li><a target="reports" id="hrefReports" href="~/Reports/VendorReports"><i class="fa fa-file-text-o user01"></i> <span>Reports</span></a></li>
                        </ul>
