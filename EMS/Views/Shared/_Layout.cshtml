                            <input type="checkbox" id="ChkBatch" onchange="funBatchNo();" style="padding: 10%;" maxlength="12">
                            <a href="#" style="color: blue !important;"><u><span id="spnBatchNumber">AA160507</span></u></a>
                            <input type="text" id="txtBatchNumber" style="display:none;width: 75%;" onblur="funChangeBatchNumber();" onkeypress="return isNumberKey(event)"
