﻿<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />

    <link href="~/Scripts/MultiSelectDropDown/jquery.multiselect.css" rel="stylesheet" />
    <link href="~/Content/css/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/Content/AtlasUtility.css" rel="stylesheet" />
</head>
<script> var Titlejs = '@ViewBag.Title' </script>
<body class="skin-blue sidebar-mini" id="ToggleNavigationbodyID">
    <div class="app-body" id="app-body">
        <div class="wrapper" id="wrapper_main">
            <header class="main-header">
                <!-- Logo -->
                <a href="~/AccountPayable/Vendors" class="logo">
                    <!-- mini logo for sidebar mini 50x50 pixels -->
                    <span class="logo-mini"><!--<b>A</b>LT--> <img src="~/Content/images/logoMini.png" alt="logo"></span>
                    <!-- logo for regular state and mobile devices -->
                    <span class="logo-lg"><!--<b>Admin</b>LTE--><img src="~/Content/images/logo.png" alt="logo" style="width:87%;"></span>
                </a>
                <!-- Header Navbar: style can be found in header.less -->
                <nav class="navbar navbar-static-top" role="navigation">
                    <!-- Sidebar toggle button-->
                    <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button" id="NavigationToggle">
                        <span class="sr-only">Toggle navigation</span>
                    </a>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
                        <ul class="nav navbar-nav topMenu" id="navMainUL">
                            <li><a id="hrefPayable" href="~/AccountPayable/Vendors"><i class="fa fa-money"></i> <span>Accounts Payable</span></a></li>
                            <li><a id="hrefPayroll" href="~/Payroll/LoadPayroll"><i class="fa fa-usd user01"></i> <span>Payroll</span></a></li>
                            <li><a id="hrefCRW" onclick="AtlasNavigation.toModule('CRW'); return false;" href="javascript: AtlasNavigation.toModule('CRW'); return false;"><i class="fa fa-calculator"></i> <span>CRW</span></a></li>
                            @*<li><a id="hrefLedger" href="~/Ledger/TrailBalance"><i class="fa fa-folder-open user01"></i> <span>Ledger</span></a></li>*@
                            <li><a id="hrefLedger" href="~/Ledger/JEDistributionChange"><i class="fa fa-folder-open user01"></i> <span>Ledger</span></a></li>
                            <li><a id="hrefBudget" onclick="AtlasNavigation.toModule('Budget'); return false;" href="~/Budget/Import"><i class="fa fa-suitcase user01"></i> <span>Budget</span></a></li>
                            <li><a id="hrefReports" href="~/Reports/VendorReports"><i class="fa fa-file-text-o user01"></i> <span>Reports</span></a></li>
                        </ul>
                    </div><!-- /.navbar-collapse -->
                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav">
                            @*<li class="userText">EMS Review</li>*@
                            <li class="dropdown user user-menu" title="Profile" id="user-menu-user">
                                <a href="~/UserProfile/MyDefaults">
                                    <i class="fa fa-user user02"></i>
                                </a>
                            </li>
                            <li class="dropdown user user-menu " title="Settings">
                                <a href="~/Setting/Company" class="">
                                    <i id="CompanyIconI" class="fa fa-cog user02 "></i>
                                </a>
                            </li>
                            <li class="dropdown user user-menu" title="Logout">
                                <a href="#" class="logout">
                                    <i class="fa fa-power-off user02"></i>
                                </a>
                            </li>
                            <!-- Control Sidebar Toggle Button -->
                        </ul>
                    </div>
                </nav>
            </header>
            <!-- Left side column. contains the logo and sidebar -->
            <aside class="main-sidebar">
                <!-- sidebar: style can be found in sidebar.less -->
                <section class="sidebar" style="height: auto;">

                        <ul class="sidebar-menu" id="UlListReports">
                            <li class=" treeview" id="LiVendorsReports">
                                <a href="~/Reports/VendorReports">
                                    <i class="fa fa-user"></i>
                                    <span>Vendors</span>
                                </a>
                            </li>
                            <li class="treeview" id="LiPurchaseReports">
                                <a href="~/Reports/POReports">
                                    <i class="fa fa-briefcase"></i>
                                    <span>Purchase Orders</span>
                                </a>
                            </li>
                            <li class="treeview" id="LiAPInvoiceReports">
                                <a href="~/Reports/APInvoiceReports">
                                    <i class="fa fa-money">
                                    </i> <span>Invoices</span>
                                </a>
                            </li>
                            <li class="treeview" id="LiReportPettyCash">
                                <a href="~/Reports/PettyCash">
                                    <i class="fa fa-envelope-o"></i>
                                    <span>Petty Cash</span>
                                </a>
                            </li>
                            <li class="treeview" id="LiReportBanking">
                                <a href="~/Reports/CheckPrint">
                                    <i class="fa fa-bank"></i>
                                    <span>Banking</span>
                                </a>
                            </li>
                            <li class="treeview" id="LiReportPayroll">
                                <a href="~/Reports/PayEditReport">
                                    <i class="fa fa-dollar"></i>
                                    <span>Payroll</span>
                                </a>
                            </li>
                            <li class="treeview" id="LiCostReport">
                                <a href="~/Reports/CostReportSheet">
                                    <i class="fa fa-calculator"></i>
                                    <span>Cost Report</span>
                                </a>
                            </li>
                            <li class="treeview" id="LiReportsledgers">
                                <a href="~/Reports/Reportsledgers">
                                    <i class="fa fa-folder-open"></i>
                                    <span>Ledger</span>
                                </a>
                            </li>
                            <li class="treeview" id="LiSetupReports">
                                <a href="~/Reports/SetUp">
                                    <i class="fa fa-gears"></i>
                                    <span>Setup</span>
                                </a>
                            </li>

                        </ul>

                    <div id="DvMasterBatch" style="margin-top: 90%; margin-left: 10%; ">

                        <p style="padding-left: 10%; padding-top: 4%; width: 90%; background-color: white; border: 2px solid #a1a1a1; border-radius: 20px;">
                            <b> <span>Batch Number</span></b><br>
                            <input type="checkbox" id="ChkBatch" onchange="funBatchNo();" style="padding: 10%;" maxlength="12">
                            <a href="#" style="color: blue !important;"><u><span id="spnBatchNumber">AA160507</span></u></a>
                            <input type="text" id="txtBatchNumber" style="display:none;width: 75%;" onblur="funChangeBatchNumber();" onkeypress="return isNumberKey(event)"
                                   maxlength="12">
                            <input type="text" style="visibility:hidden">
                        </p>

                    </div>

                    <div id="dvProductionname" class="userText01" style="background-color: #4CBF63; width: 80%; margin-left: 10%; ">
                        <span id="productionname" style=" font-size: 13px; color: #fff; vertical-align: 1%; text-align: center !important;margin-left: 35%; "></span>
                    </div>

                </section>
                <!-- /.sidebar -->
            </aside>
            <!-- Content Wrapper. Contains page content -->
            <div id="Atlas_Content" class="AtlasAccess content-wrapper" style="min-height: 353px;">
                <!-- Content Header (Page header) -->
                @RenderSection("BreadCrumb", required: false)
                <div id="showMSG" style="display:block;"></div>
                @RenderSection("Body", required: false)
                <!-- Main content -->
                <!-- /.content -->
            </div>
            <div id="Atlas_Access" class="AtlasAccess content-wrapper">
                You do not have permission to access this module. <br />
                Please contact your System Administrator if you need access to this module.
            </div>
        </div>
        <div id="preload" style="display: none;width: 100%;margin: auto 0px;text-align: center;top: 20%;">
            <img id="dvLoader" src="~/Images/InternetSlowdown_Day.gif" style="width:480px;" />
        </div>
        <!--suppier popup End-->
        <div id="fade" class="black_overlay"></div>
        <!-- jQuery 2.1.4 -->
        <script src="~/Content/js/jquery-2.2.0.min.js"></script>
        @*<script src="~/Content/js/jQuery-2.1.4.min.js"></script>*@
        <!-- jQuery UI 1.11.4 -->
        <script src="~/Content/js/jquery-ui.min.js"></script>
        <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
        <script>
            $.widget.bridge('uibutton', $.ui.button);
        </script>
        <!-- Bootstrap 3.3.5 -->
        <script src="~/Content/js/bootstrap.min.js"></script>
        <!-- Slimscroll -->
        <script src="~/Content/js/jquery.slimscroll.min.js"></script>
        <!-- AdminLTE App -->
        <script src="~/Content/js/app.min.js"></script>
        <script src="~/Scripts/utility.js"></script>
        <script src="~/Scripts/AtlasUtilities.js"></script>
        <script src="~/Scripts/timeout-dialog.js"></script>
        <script src="~/Scripts/notify.js"></script>
        @*<script src="~/Content/js/jquery-ui.min.js"></script>*@
        <link href="~/Content/js/jquery-ui.structure.css" rel="stylesheet" />
        <link href="~/Content/js/jquery-ui.structure.min.css" rel="stylesheet" />
        <link href="~/Content/js/jquery-ui.theme.min.css" rel="stylesheet" />
        <script src="~/Content/js/jquery-ui.min.js"></script>
        @*<link href="~/Content/DataTable/dataTables.responsive.css" rel="stylesheet" />
            <script src="~/Content/DataTable/dataTables.responsive.js"></script>*@
        <script src="~/Scripts/MultiSelectDropDown/jquery.multiselect.js"></script>
        <script src="~/Scripts/Validation/Validation.js"></script>
        <script src="~/Scripts/jquery.mask.min.js"></script>
        @RenderSection("Javascript", required: false)
        <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
        <style>
            .ui-state-hover, .ui-widget-content .ui-state-hover, .ui-widget-header .ui-state-hover, .ui-state-focus, .ui-widget-content .ui-state-focus, .ui-widget-header .ui-state-focus {
                border: 1px solid #999999 !important;
                background: #dadada url("images/ui-bg_glass_75_dadada_1x400.png") 50% 50% repeat-x !important;
                font-weight: normal !important;
                color: #212121 !important;
            }
        </style>
    </div>
</body>
</html>
