<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>EMS - @ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />

    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <link href="~/Scripts/MultiSelectDropDown/jquery.multiselect.css" rel="stylesheet" />
    <link href="~/Content/css/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/Content/AtlasUtility.css" rel="stylesheet" />
</head>
<body class="skin-blue sidebar-mini">

    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="~/AccountPayable/Vendors" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><!--<b>A</b>LT--> <img src="~/Content/images/logoMini.png" alt="logo"></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><!--<b>Admin</b>LTE--><img src="~/Content/images/logo.png" alt="logo" style="width:87%;"></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
                    <ul class="nav navbar-nav topMenu" id="navMainUL">
                        <li><a id="hrefPayable" href="~/AccountPayable/Vendors"><i class="fa fa-money"></i> <span>Accounts Payable</span></a></li>
                        <li><a id="hrefPayroll" href="~/Payroll/LoadPayroll"><i class="fa fa-usd user01"></i> <span>Payroll</span></a></li>
                        <li><a id="hrefCRW" onclick="AtlasNavigation.toModule('CRW'); return false;" href="~/CRW/CRWList"><i class="fa fa-users "></i> <span>CRW</span></a></li>
                        <li><a id="hrefLedger" href="~/Ledger/TrailBalance"><i class="fa fa-folder-open user01"></i> <span>Ledger</span></a></li>
                        <li><a id="hrefBudget" onclick="AtlasNavigation.toModule('Budget'); return false;" href="~/Budget/Import"><i class="fa fa-suitcase user01"></i> <span>Budget</span></a></li>
                        <li><a id="hrefReports" href="~/Reports/VendorReports"><i class="fa fa-file-text-o user01"></i> <span>Reports</span></a></li>
                    </ul>
                </div><!-- /.navbar-collapse -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li class="dropdown user user-menu">
                            <a href="#" class="">
                                <i class="fa fa-user user02"></i>
                            </a>
                        </li>
                        <li class="dropdown user user-menu ">
                            <a href="~/Setting/Company" class="">
                                <i id="CompanyIconI" class="fa fa-cog user02 "></i>
                            </a>
                        </li>
                        <li class="dropdown user user-menu">
                            <a href="#" class="logout">
                                <i class="fa fa-power-off user02"></i>
                            </a>
                        </li>
                        <!-- Control Sidebar Toggle Button -->
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar" style="height: auto;">
              
               
                    <ul id="UlPayroll" class="sidebar-menu">
                        <li id="LiPayrollLoad">
                            <a href="#">
                                <i class="fa fa-building-o"></i> <span>Load</span>
                            </a>
                        </li>
                        <li id="LiPayrollAudit" class="treeview">
                            <a href="#">
                                <i class="fa fa-user"></i>
                                <span>Audit</span>
                            </a>
                        </li>
                        <li id="LiPayrollRCP" class=" treeview">
                            <a href="#">
                                <i class="fa fa-briefcase"></i> <span>RCP</span>
                            </a>
                        </li>
                        <li id="LiPayrollSetup" class="treeview">
                            <a href="#">
                                <i class="fa fa-list-alt"></i>
                                <span>Setup</span>

                            </a>
                        </li>

                    </ul>
                
              
               
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div id="Atlas_Content" class="AtlasAccess content-wrapper" style="min-height: 353px;">
            <!-- Content Header (Page header) -->
            @RenderSection("BreadCrumb", required: false)
            <div id="showMSG" style="display:block;"></div>
            @RenderSection("Body", required: false)
            <!-- Main content -->
            <!-- /.content -->
        </div>
        <div id="Atlas_Access" class="AtlasNoAccess content-wrapper">
            You do not have permission to access this module. <br />
            Please contact your System Administrator if you need access to this module.
        </div>
    </div>
    <!--suppier popup End-->
    <div id="fade" class="black_overlay"></div>
    <!-- jQuery 2.1.4 -->
    <script src="~/Content/js/jQuery-2.1.4.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Content/js/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.5 -->
    <script src="~/Content/js/bootstrap.min.js"></script>
    <!-- Slimscroll -->
    <script src="~/Content/js/jquery.slimscroll.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/js/app.min.js"></script>
    <script src="~/Scripts/utility.js"></script>
    <script src="~/Scripts/AtlasUtilities.js"></script>
    @*<script src="~/Content/js/jquery-ui.min.js"></script>*@

    <link href="~/Content/js/jquery-ui.structure.css" rel="stylesheet" />
    <link href="~/Content/js/jquery-ui.structure.min.css" rel="stylesheet" />
    <link href="~/Content/js/jquery-ui.theme.min.css" rel="stylesheet" />
    <script src="~/Content/js/jquery-ui.min.js"></script>
    @*<link href="~/Content/DataTable/dataTables.responsive.css" rel="stylesheet" />
        <script src="~/Content/DataTable/dataTables.responsive.js"></script>*@
    <script src="~/Scripts/MultiSelectDropDown/jquery.multiselect.js"></script>
    <script src="~/Scripts/Validation/Validation.js"></script>
    <script src="~/Scripts/jquery.mask.min.js"></script>
    @RenderSection("Javascript", required: false)
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->

</body>
</html>
